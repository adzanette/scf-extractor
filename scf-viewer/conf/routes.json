{
   "index":{
      "pattern":"",
      "controller":"SiteController",
      "method":"index"
   },
   "404":{
      "pattern":"404",
      "controller":"SiteController",
      "method":"show404"
   },
   "verb-list":{
      "pattern":"verb-list/{corpus}/{page}",
      "controller":"SiteController",
      "method":"showVerbList",
      "defaults":{
         "page":1
      },
      "restrictions":{
         "page":"\\d+"
      }
   },
   "frame-list":{
      "pattern":"frame-list/{corpus}/{verbId}/{verbPage}/{page}",
      "controller":"SiteController",
      "method":"showFrameList",
      "defaults":{
         "page":1
      },
      "restrictions":{
         "page":"\\d+",
         "verbPage":"\\d+",
         "verbId":"\\d+"
      }
   },
   "example-list":{
      "pattern":"example-list/{corpus}/{verbId}/{verbPage}/{frameId}/{framePage}/{page}",
      "controller":"SiteController",
      "method":"showExampleList",
      "defaults":{
         "page":1
      },
      "restrictions":{
         "page":"\\d+",
         "verbPage":"\\d+",
         "verbId":"\\d+",
         "framePage":"\\d+",
         "frameId":"\\d+"
      }
   },
   "semantic-frames-list":{
      "pattern":"semantic-frames-list/{corpus}/{page}",
      "controller":"SiteController",
      "method":"showSemanticFramesList",
      "defaults":{
         "page":1
      },
      "restrictions":{
         "page":"\\d+"
      }
   },
   "sematic-frames":{
      "pattern":"frame-list/{corpus}/{verbId}/{verbPage}/{page}",
      "controller":"SiteController",
      "method":"showFrameList",
      "defaults":{
         "page":1
      },
      "restrictions":{
         "page":"\\d+",
         "verbPage":"\\d+",
         "verbId":"\\d+"
      }
   }
}